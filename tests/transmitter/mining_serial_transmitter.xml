<root>

    <tracereader name="mangroveTraceReader">
        <variables path="transmitter/trace.variables"/>
        <traces>
            <trace path="transmitter/trace.mangrove"/>
        </traces>
    </tracereader>

    <coneminer name="manualDefinition">
        <directions>
            <direction name="rst" dir="in"/>
            <direction name="data_to_send[0]" dir="out"/>
            <direction name="data_to_send[1]" dir="out"/>
            <direction name="data_to_send[2]" dir="out"/>
            <direction name="data_to_send[3]" dir="out"/>
            <direction name="data_to_send[4]" dir="out"/>
            <direction name="data_to_send[5]" dir="out"/>
            <direction name="data_to_send[6]" dir="out"/>
            <direction name="data_to_send[7]" dir="out"/>
            <direction name="send" dir="in"/>
            <direction name="val" dir="out"/>
        </directions>
        <coneOfInfluence name="cone1">
            <atomicPropositions>
                <atomicProposition formula="send" dir="in"/>
                <atomicProposition formula="val" dir="out"/>
                <atomicProposition formula="rst" dir="in"/>
            </atomicPropositions>
        </coneOfInfluence>

    </coneMiner>

    <propositionminer name="TBMiner">
        <ConsequentTemplates>
            <template value="p1 U p2"/>
            <template value="p1 R p2"/>
            <template value="p1"/>
        </ConsequentTemplates>
    </propositionminer>

    <propertyminer name="A-teamMiner">
        <templates>
            <template value="G( p1 -> (p2) )"/>
        </templates>
        <constraints maxVariables="10"/>
    </propertyminer>

    <propertyprinter name="psl"/>

</root>
