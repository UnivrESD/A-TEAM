<root>

    <tracereader name="mangroveTraceReader">
        <variables path="testUntil/trace.variables"/>
        <traces>
            <trace path="testUntil/trace.mangrove"/>
        </traces>
    </tracereader>

        
    <coneminer name="manualDefinition">

        <coneOfInfluence name="cone1">
        <directions>
        </directions>

            <atomicPropositions>
                <atomicProposition formula="e" dir="in"/>
                <atomicProposition formula="f" dir="in"/>
                <atomicProposition formula="g" dir="out"/>
                <atomicProposition formula="h" dir="out"/>

                <!--
                <atomicProposition formula="~c <=_ ~d" dir="out"/>
                <atomicProposition formula="(X[1]i = X[1]h)" dir="in"/>
                <atomicProposition formula="a" dir="out"/>
                -->
            </atomicPropositions>
        </coneOfInfluence>

    </conemine>

    <propositionminer name="TBMiner">
        <ConsequentTemplates>
            <template value="p1"/>
<!--
            <template value="p1 && p2"/>
            <template value="p1 || p2"/>
            <template value="p1 U p2"/>
            <template value="p1 R p2"/>
            <template value="p1"/>
     -->
        </ConsequentTemplates>
    </propositionminer>

    <propertyminer name="A-teamMiner">
        <constraints maxVariables="1"/>
        <!--        <faultyTraces list="testUntil/faultyTraces/traces.xml"/> -->
    </propertyminer>

    <!-- <propertyQualifier name="FBQualifier"/> -->

    <propertyprinter name="psl"/>

</root>
