<root>

    <tracereader name="mangroveTraceReader">
        <variables path="camellia/trace.variables"/>
        <traces>
            <trace path="camellia/trace.mangrove"/>
        </traces>
    </tracereader>

    <coneminer name="manualDefinition">
        <directions>
            <direction name="rstn" dir="in"/>
            <direction name="en" dir="in"/>
            <direction name="drdy" dir="in"/>
            <direction name="krdy" dir="in"/>
            <direction name="bsy" dir="out"/>
            <direction name="kvld" dir="out"/>
            <direction name="dvld" dir="out"/>
        </directions>
    </coneMiner>

    <propositionminer name="TBMiner">
        <ConsequentTemplates>
            <template value="p1 U p2"/>
            <template value="p1 R p2"/>
            <template value="p1"/>
        </ConsequentTemplates>
    </propositionminer>

    <propertyminer name="A-teamMiner">
        <templates>
            <template value="G( p1 -> (p2) )"/>
        </templates>
        <constraints maxVariables="10"/>
    </propertyminer>

    <propertyprinter name="psl"/>

</root>
